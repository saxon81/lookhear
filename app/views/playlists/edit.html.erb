<h2>We found 5 songs with lyrics containing these concepts found in your photo:</h2>
<h3><%= @lyrics.titleize.split(' ').to_sentence %></h3>
<ul>
	<% @results.each do |result| %>
		<li>
			<%= link_to "#{result[:track_name]} by #{result[:artist_name]}", add_song_playlist_path(@playlist, {track_name: result[:track_name], artist_name: result[:artist_name]}) %>
				
		</li>
	<% end %>
</ul>
